{
  "name": "all-trees-tracker",
  "version": "0.1.0",
  "description": "A tracker mod for the Breath of the Wild All Trees challenge run",
  "type": "module",
  "packageManager": "bun@1.2.20",
  "license": "GPL-3.0-or-later",
  "repository": "https://github.com/Makonede/all-trees-tracker",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "tauri": "NO_STRIP=true tauri",
    "lint": "biome check --write"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.7.0",
    "@tauri-apps/plugin-opener": "^2.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@fontsource-variable/roboto": "^5.2.6",
    "@lucide/svelte": "^0.525.0",
    "@skeletonlabs/skeleton": "^3.1.7",
    "@skeletonlabs/skeleton-svelte": "^1.3.1",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.25.1",
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@tauri-apps/cli": "^2.7.1",
    "@types/bun": "^1.2.19",
    "@types/node": "^24.1.0",
    "sveaflet": "^0.1.4",
    "svelte": "^5.36.13",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^4.3.0",
    "tailwindcss": "^4.1.11",
    "tslib": "^2.8.1",
    "turbo": "^2.5.5",
    "typescript": "~5.8.3",
    "vite": "^7.0.5"
  },
  "devEngines": {
    "runtime": {
      "name": "bun",
      "onFail": "error"
    },
    "packageManager": {
      "name": "bun",
      "onFail": "error"
    }
  },
  "trustedDependencies": ["@tailwindcss/oxide"]
}
