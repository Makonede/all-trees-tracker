{
  "name": "all-trees-tracker",
  "version": "0.1.0",
  "description": "A tracker mod for the Breath of the Wild All Trees challenge run",
  "type": "module",
  "packageManager": "bun@1.3.4",
  "license": "GPL-3.0-or-later",
  "repository": "https://github.com/Makonede/all-trees-tracker",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "tauri": "NO_STRIP=true tauri",
    "lint": "biome check --write"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.9.0",
    "@tauri-apps/plugin-opener": "^2.5.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@fontsource-variable/fira-code": "^5.2.7",
    "@fontsource-variable/roboto": "^5.2.9",
    "@lucide/svelte": "^0.555.0",
    "@skeletonlabs/skeleton": "^4.6.0",
    "@skeletonlabs/skeleton-svelte": "^4.6.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.49.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@sveltekit-i18n/parser-icu": "^1.0.8",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "@tauri-apps/cli": "^2.9.4",
    "@toolwind/anchors": "^1.0.10",
    "@types/bun": "^1.3.3",
    "@types/node": "^24.10.1",
    "flowbite": "^4.0.1",
    "flowbite-svelte": "^1.28.1",
    "layerchart": "^2.0.0-next.43",
    "sveaflet": "^0.1.4",
    "svelte": "^5.45.2",
    "svelte-adapter-bun": "^1.0.1",
    "svelte-check": "^4.3.4",
    "sveltekit-i18n": "^2.4.2",
    "tailwindcss": "^4.1.17",
    "tslib": "^2.8.1",
    "turbo": "^2.6.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  },
  "devEngines": {
    "runtime": {
      "name": "bun",
      "onFail": "error"
    },
    "packageManager": {
      "name": "bun",
      "onFail": "error"
    }
  },
  "trustedDependencies": ["@tailwindcss/oxide", "@toolwind/anchors", "esbuild"]
}
